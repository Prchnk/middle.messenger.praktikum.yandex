{
	"compilerOptions": {
		"module": "es2015",
		"target": "ES5",
		"lib": [
			"ES2019",
			"DOM"
		],
		"noImplicitAny": true,
		"noUnusedLocals": true,
		"noUnusedParameters": true,
		"sourceMap": true,
